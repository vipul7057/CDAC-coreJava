package day2;

import java.util.Scanner;

public class Q20 {
	public static void main(String args[])
	{
		Scanner c = new Scanner(System.in);
		System.out.println("Enter two numbers");
		int n1 = c.nextInt();
		int n2 = c.nextInt();
	        while(n1 < n2)
	        {
	        	int f = 0;
	        	for(int i = 2 ; i < n1 / 2 ;i++)
	        	{
	        		if(n1 % i == 0)
	        		{
	        			f = 1;
	        			break;
	        		}
	        	}
	        	if(f == 0)
	        	{
	        		System.out.print(n1 + " ");
	        	}
	        	n1++;
	        }
	        
	    
	}
}

