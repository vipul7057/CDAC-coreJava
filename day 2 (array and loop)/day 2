package day2;
import java.util.*;
public class Q16 {

	public static void main(String a[])
	{
		Scanner c = new Scanner(System.in);
		System.out.println("Enter the number");
		int n = c.nextInt();
		int sum = 1;
		for(int i = 1 ; i <= 10 ;i++)
		{	
			sum = n * i;
			System.out.println(sum);
		}
	}
	
}
