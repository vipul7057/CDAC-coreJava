package day2;
import java.util.Scanner;
public class Q19 {
	public static void main(String args[])
	{
		int num = 2;
		int finalsum = 0;
		int n = 10;
		for(int i = 0 ; i < n ;i ++)
		{
			num = num + 10;
			if(i == n - 1)
			{
				System.out.println(num);
				break;
			}	
			System.out.print(num+"+");
			finalsum +=num;
		}
		System.out.println();
		System.out.println("Sum is "+finalsum);
	}
}
